PLATFORM=unix

ifneq ("$(windir)","")
PLATFORM=windows
endif

ifneq ("$(WinDir)","")
PLATFORM=windows
endif

ifneq ("$(WINDIR)","")
PLATFORM=windows
endif

UNAME=$(shell uname -o)

ifeq ("$(UNAME)","Cygwin")
PLATFORM=cygwin
endif

ifeq ("$(BUILD_CONFIGURATION)","")
	BUILD_CONFIGURATION=ReleaseLinux
endif

ifeq ("$(BUILD_CONFIGURATION)","DebugLinux")
CC= gcc
CXX= g++
AR= ar
CFLAGS= -g -I ../ -D_DEBUG
CXXFLAGS= -g -std=c++0x -I ../ -D_DEBUG
ARFLAGS= cru
LDFLAGS= -shared
RESULT=libRender3DD.so
INTERMEDIATE_DIR=DebugLinux/obj
DESTINATION_DIR=../Bin
ifeq ("$(PLATFORM)","cygwin")
	RM=rm -f
	MKDIR=mkdir -p
	INTERMEDIATE_DIR_MK=$(INTERMEDIATE_DIR)
	DESTINATION_DIR_MK=$(DESTINATION_DIR)
else
	RM= rm -f
	MKDIR= mkdir -p
	INTERMEDIATE_DIR_MK=DebugLinux/obj
	DESTINATION_DIR_MK=../Bin
endif

endif

ifeq ("$(BUILD_CONFIGURATION)","ReleaseLinux")
CC= gcc
CXX= g++
AR= ar
CFLAGS= -g -O2 -I ../
CXXFLAGS= -g -std=c++0x -O2 -I ../
ARFLAGS= cru
LDFLAGS= -shared
RESULT=libRender3D.so
INTERMEDIATE_DIR=ReleaseLinux/obj
DESTINATION_DIR=../Bin
ifeq ("$(PLATFORM)","cygwin")
	RM=rm -f
	MKDIR=mkdir -p
	INTERMEDIATE_DIR_MK=$(INTERMEDIATE_DIR)
	DESTINATION_DIR_MK=$(DESTINATION_DIR)
else
	RM= rm -f
	MKDIR= mkdir -p
	INTERMEDIATE_DIR_MK=ReleaseLinux/obj
	DESTINATION_DIR_MK=../Bin
endif

endif

SOURCES= BaseNode.cpp Chopper.cpp Material.cpp NodeBlur.cpp NodeBoolean.cpp NodeBooleanTime.cpp NodeComplexMesh.cpp NodeComplexMesh2.cpp NodeDecal.cpp NodeDetail.cpp NodeFlare.cpp NodeGlare.cpp NodeLight.cpp NodeMesh.cpp NodeParticleField.cpp NodeParticleSystem.cpp NodeParticleSystem2.cpp NodePath.cpp NodeScale.cpp NodeScene.cpp NodeSound.cpp NodeSpin.cpp NodeSplinePath.cpp NodeSprite.cpp NodeStarField.cpp NodeTransform.cpp PathMap.cpp RenderContext.cpp Scene.cpp Stripper.cpp

SOURCEPATHS= BaseNode.cpp Chopper.cpp Material.cpp NodeBlur.cpp NodeBoolean.cpp NodeBooleanTime.cpp NodeComplexMesh.cpp NodeComplexMesh2.cpp NodeDecal.cpp NodeDetail.cpp NodeFlare.cpp NodeGlare.cpp NodeLight.cpp NodeMesh.cpp NodeParticleField.cpp NodeParticleSystem.cpp NodeParticleSystem2.cpp NodePath.cpp NodeScale.cpp NodeScene.cpp NodeSound.cpp NodeSpin.cpp NodeSplinePath.cpp NodeSprite.cpp NodeStarField.cpp NodeTransform.cpp PathMap.cpp RenderContext.cpp Scene.cpp Stripper.cpp

HEADERS= BaseNode.h Chopper.h Material.h NodeBlur.h NodeBoolean.h NodeBooleanTime.h NodeComplexMesh.h NodeComplexMesh2.h NodeDecal.h NodeDetail.h NodeFlare.h NodeGlare.h NodeLight.h NodeMesh.h NodePGE.h NodeParticleField.h NodeParticleSystem.h NodeParticleSystem2.h NodePath.h NodeScale.h NodeScene.h NodeSound.h NodeSpin.h NodeSplinePath.h NodeSprite.h NodeStarField.h NodeTransform.h PGE.h PathMap.h Render3dDll.h RenderContext.h Scene.h Stripper.h

VPATH= $(INTERMEDIATE_DIR)

OBJECTNAMES=$(patsubst %.c,%.o,$(patsubst %.cpp,%.o,$(SOURCES)))
OBJECTPATHS=$(addprefix $(INTERMEDIATE_DIR)/,$(OBJECTNAMES))

$(DESTINATION_DIR)/$(RESULT): $(OBJECTNAMES)
ifeq ("$(wildcard $(DESTINATION_DIR) )","")
	-$(MKDIR) $(DESTINATION_DIR_MK)
endif
	$(CXX) -shared $(CXXFLAGS) $(SYSROOTFLAGS) $(OBJECTPATHS) -o $@ $(LDFLAGS)

$(INTERMEDIATE_DIR)/Render3D.d: $(SOURCES) $(HEADERS)
ifeq ("$(wildcard $(DESTINATION_DIR) )","")
	-$(MKDIR) $(DESTINATION_DIR_MK)
endif
ifeq ("$(wildcard $(INTERMEDIATE_DIR) )","")
	-$(MKDIR) $(INTERMEDIATE_DIR_MK)
endif
	$(CXX) $(CXXFLAGS) -MM $(SOURCEPATHS) > $@

-include $(INTERMEDIATE_DIR)/Render3D.d

clean:
	$(RM) $(DESTINATION_DIR)/$(RESULT)
	$(RM) $(INTERMEDIATE_DIR)/Render3D.d
	$(RM) $(OBJECTPATHS)

%.o: %.cpp
	$(CXX) -fPIC $(CXXFLAGS) -c -o $(INTERMEDIATE_DIR)/$@ $<

%.o: %.c
	$(CC) -fPIC $(CFLAGS) -c -o $(INTERMEDIATE_DIR)/$@ $<
